-- üéØ SISTEMA DE FARMING 3 MUNDOS
-- ============================================
local FarmingSystem = {
    Enabled = false,
    AutoAttack = true,
    CurrentSea = 1,
    CurrentTarget = nil,
    AttackSpeed = 0.3,
    GoalLevel = 2800  -- At√© o terceiro mundo
}

-- Get player level
function FarmingSystem:GetPlayerLevel()
    local leaderstats = Player:FindFirstChild("leaderstats")
    if leaderstats then
        local level = leaderstats:FindFirstChild("Level")
        if level then
            return level.Value or 1
        end
    end
    return 1
end

-- Determine current sea based on level
function FarmingSystem:GetCurrentSea()
    local level = self:GetPlayerLevel()
    
    if level < 700 then
        return 1
    elseif level < 1500 then
        return 2
    else
        return 3
    end
end

-- Get NPCs for current sea
function FarmingSystem:GetCurrentNPCs()
    local sea = self:GetCurrentSea()
    self.CurrentSea = sea
    
    if sea == 1 then
        return AllNPCs.Sea1
    elseif sea == 2 then
        return AllNPCs.Sea2
    else
        return AllNPCs.Sea3
    end
end

-- Get next level goal
function FarmingSystem:GetNextGoal()
    local level = self:GetPlayerLevel()
    
    if level < 700 then
        return 700
    elseif level < 1500 then
        return 1500
    else
        return 2800
    end
end

-- Update display
function FarmingSystem:UpdateDisplay()
    local level = self:GetPlayerLevel()
    local sea = self:GetCurrentSea()
    local nextGoal = self:GetNextGoal()
    local progress = math.floor((level / nextGoal) * 100)
    
    LevelLabel.Text = "LEVEL: " .. level .. " | SEA: " .. sea
    ProgressLabel.Text = string.format("PROGRESS: %d%% | NEXT GOAL: %d", progress, nextGoal)
    
    if self.CurrentTarget then
        FarmingLabel.Text = string.format("FARMING: %s | TARGET: %s", 
            self.Enabled and "ON" or "OFF", 
            self.CurrentTarget.Name)
    else
        FarmingLabel.Text = string.format("FARMING: %s | TARGET: None", 
            self.Enabled and "ON" or "OFF")
    end
    
    -- Color progress based on sea
    if sea == 1 then
        ProgressLabel.TextColor3 = Color3.fromRGB(100, 200, 255)
    elseif sea == 2 then
        ProgressLabel.TextColor3 = Color3.fromRGB(255, 150, 50)
    else
        ProgressLabel.TextColor3 = Color3.fromRGB(200, 50, 200)
    end
end

-- Get recommended NPC
function FarmingSystem:GetRecommendedNPC()
    local level = self:GetPlayerLevel()
    local npcs = self:GetCurrentNPCs()
    
    for i = #npcs, 1, -1 do
        local npc = npcs[i]
        if level >= npc.MinLevel and level <= npc.MaxLevel + 50 then
            return npc.Name
        end
    end
    
    return npcs[1].Name
end

-- Find enemy
function FarmingSystem:FindEnemy()
    local targetType = self:GetRecommendedNPC()
    local character = Player.Character
    if not character or not character:FindFirstChild("HumanoidRootPart") then return nil end
    
    local closest = nil
    local closestDist = 500
    local charPos = character.HumanoidRootPart.Position
    
    for _, npc in pairs(Workspace:GetChildren()) do
        if npc:FindFirstChild("Humanoid") and npc.Humanoid.Health > 0 then
            if npc.Name:find(targetType) then
                local root = npc:FindFirstChild("HumanoidRootPart")
                if root then
                    local dist = (charPos - root.Position).Magnitude
                    if dist < closestDist then
                        closest = npc
                        closestDist = dist
                    end
                end
            end
        end
    end
    
    return closest, closestDist, targetType
end

-- Auto attack
function FarmingSystem:AutoAttack()
    if not self.AutoAttack then return end
    
    -- Attack with E key
    VirtualInputManager:SendKeyEvent(true, "E", false, game)
    task.wait(0.05)
    VirtualInputManager:SendKeyEvent(false, "E", false, game)
    
    -- Skill with F key
    VirtualInputManager:SendKeyEvent(true, "F", false, game)
    task.wait(0.05)
    VirtualInputManager:SendKeyEvent(false, "F", false, game)
    
    -- Mouse click
    VirtualInputManager:SendMouseButtonEvent(0, 0, 0, true, game, 0)
    task.wait(0.05)
    VirtualInputManager:SendMouseButtonEvent(0, 0, 0, false, game, 0)
end

-- Move to enemy
function FarmingSystem:MoveToEnemy(enemy)
    local character = Player.Character
    if not character or not enemy then return end
    
    local charRoot = character:FindFirstChild("HumanoidRootPart")
    local enemyRoot = enemy:FindFirstChild("HumanoidRootPart")
    
    if charRoot and enemyRoot then
        charRoot.CFrame = CFrame.new(
            enemyRoot.Position + Vector3.new(0, 3, 5)
        )
    end
end

-- Start farming
function FarmingSystem:Start()
    self.Enabled = true
    print("[THE DOG HUB] Auto Farm 3 Worlds STARTED!")
    
    while self.Enabled and self:GetPlayerLevel() < self.GoalLevel do
        -- Update display
        self:UpdateDisplay()
        
        -- Find enemy
        local enemy, distance, targetType = self:FindEnemy()
        
        if enemy then
            self.CurrentTarget = enemy
            
            -- Move to enemy
            self:MoveToEnemy(enemy)
            
            -- Auto attack if close
            if distance < 30 then
                self:AutoAttack()
            end
            
            task.wait(self.AttackSpeed)
        else
            task.wait(1)
        end
    end
    
    self:Stop()
    print("[THE DOG HUB] Goal reached! Level 2800+")
end

function FarmingSystem:Stop()
    self.Enabled = false
    self.CurrentTarget = nil
    print("[THE DOG HUB] Farming stopped")
end

-- ============================================
-- üéÅ SISTEMA AUTO REDEEM CODES
-- ============================================
local CodeSystem = {
    Redeeming = false,
    RedeemedCount = 0
}

function CodeSystem:RedeemCode(code)
    local Remotes = ReplicatedStorage:FindFirstChild("Remotes")
    if Remotes then
        local CommF = Remotes:FindFirstChild("CommF_")
        if CommF then
            CommF:InvokeServer("RedeemCode", code)
            return true
        end
    end
    return false
end

function CodeSystem:RedeemAllCodes()
    if self.Redeeming then return 0 end
    
    self.Redeeming = true
    self.RedeemedCount = 0
    
    print("[THE DOG HUB] Redeeming " .. #BloxFruitsCodes .. " codes...")
    
    for i, code in ipairs(BloxFruitsCodes) do
        local success = self:RedeemCode(code)
        if success then
            self.RedeemedCount = self.RedeemedCount + 1
            print("[" .. i .. "/" .. #BloxFruitsCodes .. "] Redeemed: " .. code)
        else
            print("[" .. i .. "/" .. #BloxFruitsCodes .. "] Failed: " .. code)
        end
        task.wait(0.5) -- Wait between redeems
    end
    
    self.Redeeming = false
    print("[THE DOG HUB] Redeemed " .. self.RedeemedCount .. "/" .. #BloxFruitsCodes .. " codes!")
    
    return self.RedeemedCount
end

