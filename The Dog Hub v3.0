UserInputService.InputBegan:Connect(function(input)
    -- F to toggle farming
    if input.KeyCode == Enum.KeyCode.F then
        if not FarmingSystem.Enabled then
            FarmingSystem:Start()
            StartFarmBtn.Text = "‚úÖ FARMING..."
            StartFarmBtn.BackgroundColor3 = Color3.fromRGB(50, 200, 100)
            StopFarmBtn.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
        else
            FarmingSystem:Stop()
            StartFarmBtn.Text = "üöÄ START AUTO FARM (3 WORLDS)"
            StartFarmBtn.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
            StopFarmBtn.BackgroundColor3 = Color3.fromRGB(60, 60, 80)
        end
    end
    
    -- R to redeem codes
    if input.KeyCode == Enum.KeyCode.R then
        spawn(function()
            CodeSystem:RedeemAllCodes()
        end)
    end
    
    -- X to close
    if input.KeyCode == Enum.KeyCode.X then
        DogHub:Destroy()
        FarmingSystem:Stop()
    end
end)

